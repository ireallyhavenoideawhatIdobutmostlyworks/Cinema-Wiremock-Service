{
	"name": "Submit order",
	"request": {
		"method": "POST",
		"urlPathPattern": "/api/v1/order/submit",
		"bodyPatterns": [
			{
				"matchesJsonPath": "$.orderId",
				"matches": ".*"
			}
		]
	},
	"response": {
		"status": 200,
		"bodyFileName": "./order/submit-order-successful-response.json",
		"headers": {
			"Content-Type": "application/json"
		}
	}
}