{
  "name": "Register new user",
  "request": {
    "method": "POST",
    "urlPathPattern": "/api/v1/user/register",
    "bodyPatterns": [
      {
        "equalToJson": {
          "name": "mockname",
          "surname": "mockusername",
          "email": "mock@user.com",
          "password": "mockuser"
        },
        "headers" : {
          "Access-Control-Allow-Origin" : "*"
        }
      }
    ]
  },
  "response": {
    "status": 201,
    "bodyFileName": "./user/user-register-successful-response.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}