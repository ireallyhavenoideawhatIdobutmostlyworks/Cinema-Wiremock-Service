{
  "name": "Register new user",
  "request": {
    "method": "POST",
    "urlPathPattern": "/api/v1/user/register",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.name",
        "matchesJsonPath": "$.surname",
        "matchesJsonPath": "$.email",
        "matchesJsonPath": "$.password"
      }
    ]
  },
  "response": {
    "status": 201,
    "bodyFileName": "./user/user-register-ok-response.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}