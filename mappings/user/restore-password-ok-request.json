{
	"name": "Login into app",
	"request": {
		"method": "POST",
		"urlPathPattern": "/api/v1/user/restore",
		"bodyPatterns": [
			{
				"matchesJsonPath": "$.email",
				"headers" : {
					"Access-Control-Allow-Origin" : "*"
				}
			}
		]
	},
	"response": {
		"status": 201,
		"bodyFileName": "./user/restore-password-ok-response.json",
		"headers": {
			"Content-Type": "application/json"
		}
	}
}