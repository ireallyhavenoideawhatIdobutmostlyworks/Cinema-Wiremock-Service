{
  "name": "Login into app",
  "request": {
    "method": "POST",
    "urlPathPattern": "/api/v1/user/login",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.email",
        "matchesJsonPath": "$.password"
      }
    ]
  },
  "response": {
    "status": 201,
    "bodyFileName": "./user/user-login-successful-response.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
